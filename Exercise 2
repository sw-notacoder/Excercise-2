req_file <- read_html("https://en.wikipedia.org/wiki/Indiaâ€“Pakistan_cricket_rivalry")
View(req_file)
tables<-html_nodes(req_file, "table")
table1 <- html_table(tables[3], fill = TRUE)
View(table1)
table2 <- as.data.frame(table1)
View(table2)
counter=0
for(i in 1:nrow(table2))
{
  counter <- counter+table2$India[i]
}

print(counter)
#For downloading data and then performing
csvfle <- read.csv("/Users/swastik/Desktop/AMMA 2017/Code/Code_2017/Ind_Pak_rivalry.csv")
View(csvfle)
counter<- 0
match_df<- data.frame(csvfle)
View(match_df)
